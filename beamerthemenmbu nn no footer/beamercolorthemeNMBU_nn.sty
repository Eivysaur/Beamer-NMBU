\mode<presentation>

% Color definitions RGB
% NMBU colors
\definecolor{nmbugreen}{RGB}{0,154,129}
%\definecolor{nmbugreen}{cmyk}{94,0,58,0}
\definecolor{nmbulightgreen}{RGB}{209,232,223}
\definecolor{nmbublue}{RGB}{0,169,213}
\definecolor{nmbuearthblue}{RGB}{85,102,128}
\definecolor{nmbulightearthblue}{RGB}{216,230,240}
\definecolor{nmbured}{RGB}{231,82,68}
\definecolor{nmbulightred}{RGB}{252,227,219}

% Custom colors
\definecolor{framebackground}{RGB}{255,255,255}
\definecolor{softblack}{HTML}{222222}

% Add CMYK definitions to be used before printing

% Settings
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{frametitle}{fg=nmbugreen}
\setbeamercolor*{normal text}{fg=softblack}
\setbeamercolor*{caption name}{fg=nmbugreen}
\setbeamercolor*{itemize item}{fg=nmbugreen}
\setbeamercolor*{itemize subitem}{fg=nmbulightgreen}

% block color settings
\setbeamercolor*{block title}{fg=white,bg=nmbugreen}
\setbeamercolor*{block body}{parent=normal text,bg=nmbulightgreen,fg=softblack}

\setbeamercolor*{block title example}{fg=white,bg=nmbuearthblue}
\setbeamercolor*{block body example}{parent=normal text,bg=nmbulightearthblue, fg=softblack}

\setbeamercolor*{block title alerted}{fg=white,bg=nmbured}
\setbeamercolor*{block body alerted}{parent=normal text,bg=nmbulightred,fg=softblack}


